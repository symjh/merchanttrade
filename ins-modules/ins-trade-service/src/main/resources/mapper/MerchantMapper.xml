        <result column="create_time" property="createTime" />
        <result column="updater_code" property="updaterCode" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, out_merchant_id AS outMerchantId, merchant_id AS merchantId, merchant_name AS merchantName, merchant_type AS merchantType, deal_type AS dealType, support_prepayment AS supportPrepayment, settle_mode AS settleMode, mcc, trade_type_list AS tradeTypeList, pay_channel_list AS payChannelList, denied_pay_tool_list AS deniedPayToolList, auth_code AS authCode, out_trade_no AS outTradeNo, support_stage AS supportStage, partner_type AS partnerType, alipay_source AS alipaySource, wechat_channel AS wechatChannel, rate_version AS rateVersion, valid_ind AS validInd, creator_code AS creatorCode, create_time AS createTime, updater_code AS updaterCode, update_time AS updateTime
    </sql>

</mapper>
